{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": [2, "never"],
    "no-var": 2,
    "prefer-const": 2,
    "no-console": 1
  }
}
